    "obsidian-plugin",
    "wikilink",
    "display-text"
  ],
  "author": "Ivan Grubišić",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.2",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "jiti": "^2.4.2",
    "obsidian": "latest",
    "typescript": "^4.4.4"
  }
}
